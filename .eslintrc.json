{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": ["error", "double"],
    "linebreak-style": ["error","windows"],
    "eslint.packageManager": "yarn",
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }]
  }
}
